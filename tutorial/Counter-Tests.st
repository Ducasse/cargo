TestCase subclass: #CounterTest	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'Counter-Tests'!!CounterTest methodsFor: 'tests' stamp: 'ChristopheDemarey 11/28/2016 10:11'!testCanDecrementCounter	| counter |	counter := Counter new.	counter decrement.	self		assert: counter value 		equals: -1! !!CounterTest methodsFor: 'tests' stamp: 'ChristopheDemarey 11/28/2016 10:10'!testCounterInitialValueIsZero	self		assert: Counter new value 		equals: 0! !!CounterTest methodsFor: 'tests' stamp: 'ChristopheDemarey 11/28/2016 10:10'!testCanIncrementCounter	| counter |	counter := Counter new.	counter increment.	self		assert: counter value 		equals: 1! !